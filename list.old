#include "minishell.h"

t_env	*add_end(t_env *env, char *data)
{
	t_env	*node;
	t_env	*tmp;

	tmp = env;
	node = (t_env*)malloc(sizeof(t_env));
	if (!node)
		exit(-1);
	node->data = ft_strdup(data);
	node->next = NULL;
	if (!env)
		return (node);
	while (tmp->next)
		tmp = tmp->next;
	tmp->next = node;
	return (env);
}
